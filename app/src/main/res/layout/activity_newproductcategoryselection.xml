        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </FrameLayout>

    <ImageView
        android:id="@+id/accessoriesIcon"
        android:layout_width="57dp"
        android:layout_height="60dp"
        android:layout_marginTop="276dp"
        app:layout_constraintHorizontal_bias="0.858"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/addproduct_accessories_selector" />

    <TextView
        android:id="@+id/accessoriesTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="352dp"
        android:fontFamily="@font/comfortaa"
        android:text="Accessories"
        android:textColor="@color/black"
        android:textSize="12sp"
        app:layout_constraintHorizontal_bias="0.886"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <FrameLayout
        android:id="@+id/footwearFrameLayout"
        android:layout_width="97dp"
        android:layout_height="108dp"
        android:layout_marginTop="268dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@drawable/layout_border">

    </FrameLayout>

    <FrameLayout
        android:id="@+id/shirtsFrameLayout"
        android:layout_width="97dp"
        android:layout_height="108dp"
        android:layout_marginTop="116dp"
        android:background="@drawable/layout_border"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </FrameLayout>

    <FrameLayout
        android:id="@+id/skirtsFrameLayout"
        android:layout_width="97dp"
        android:layout_height="108dp"
        android:layout_marginTop="268dp"
        app:layout_constraintHorizontal_bias="0.092"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@drawable/layout_border">

    </FrameLayout>

    <FrameLayout
        android:id="@+id/trousersFrameLayout"
        android:layout_width="97dp"
        android:layout_height="108dp"
        android:layout_marginTop="116dp"
        android:background="@drawable/layout_border"
        app:layout_constraintHorizontal_bias="0.904"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </FrameLayout>

    <ImageView
        android:id="@+id/trousersIcon"
        android:layout_width="57dp"
        android:layout_height="60dp"
        android:layout_marginTop="124dp"
        app:layout_constraintHorizontal_bias="0.855"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
