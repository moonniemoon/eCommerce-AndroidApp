<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fragment_companydetails"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:tag="accountDetails"
    tools:context=".fragments.user.FragmentAccount">

    <FrameLayout
        android:id="@+id/beautyFrameLayout"
        android:layout_width="97dp"
        android:layout_height="108dp"
        android:layout_marginTop="420dp"
        android:background="@drawable/layout_border"
        app:layout_constraintHorizontal_bias="0.092"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </FrameLayout>

    <ImageView
        android:id="@+id/beautyIcon"
        android:layout_width="57dp"
        android:layout_height="60dp"
        android:layout_marginTop="428dp"
        app:layout_constraintHorizontal_bias="0.138"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/addproduct_beauty_selector" />

    <TextView
        android:id="@+id/beautyTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="500dp"
        android:fontFamily="@font/comfortaa"
        android:text="Beauty"
        android:textColor="@color/black"
        app:layout_constraintHorizontal_bias="0.152"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <FrameLayout
        android:id="@+id/accessoriesFrameLayout"
        android:layout_width="97dp"
        android:layout_height="108dp"
        android:layout_marginTop="268dp"
        android:background="@drawable/layout_border"
